{
  "name": "@tajetaje/romm-api",
  "version": "0.0.0",
  "description": "OpenAPI client for RomM",
  "author": "OpenAPI-Generator Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/jthoward64/romm-api.git"
  },
  "keywords": [
    "fetch",
    "typescript",
    "openapi-client",
    "openapi-generator"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "type": "module",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.js"
    }
  },
  "files": [
    "dist"
  ],
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "bun run build",
    "generate": "bun x @openapitools/openapi-generator-cli generate -i ./openapi.json -g typescript -o . -c ./openapi-config.json"
  },
  "dependencies": {
    "whatwg-fetch": "^3.0.0",
    "es6-promise": "^4.2.4"
  },
  "devDependencies": {
    "typescript": "^4.0"
  }
}
